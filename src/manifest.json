{
	"manifest_version": 2,

	"name": "Tabs Clustering",
	"description": "Use various data clustering methods to group your tabs together.",
	"version": "0.0.1",
	"author": "Tom Rochette <tom@tomrochette.com>",
	"homepage_url": "http://www.tomrochette.com",

	"default_locale": "en",

	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"64": "icons/icon_64.png",
		"128": "icons/icon_128.png"
	},

	"permissions": [
		"tabs"
	],

	"background": {
		"scripts": ["js/background.js"]
	},

	"browser_action": {
		"default_title": "Tabs clustering",
		"default_icon": {
			"19": "icons/icon_16.png",
			"38": "icons/icon_32.png"
		}
	},

	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}